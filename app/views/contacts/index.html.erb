<p id="notice"><%= notice %></p>

<h1>Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cpf</th>
      <th>Email</th>
      <th>Bday</th>
      <th>Zip</th>
      <th>Street</th>
      <th>Numberadrss</th>
      <th>City</th>
      <th>Nhbrhd</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.cpf %></td>
        <td><%= contact.email %></td>
        <td><%= contact.bday %></td>
        <td><%= contact.zip %></td>
        <td><%= contact.street %></td>
        <td><%= contact.numberadrss %></td>
        <td><%= contact.city %></td>
        <td><%= contact.nhbrhd %></td>
        <td><%= contact.state %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>
